<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme for API 11+. This theme completely replaces
        AppBaseTheme from res/values/styles.xml on API 11+ devices.
    -->
    <style name="AppLightOverlayTheme" parent="AppLightTheme">

        <!-- API 11 theme customizations can go here. -->
        <item name="android:windowActionBarOverlay">true</item>
    </style>

    <!-- THEMES GENERATED - http://android-ui-utils.googlecode.com/hg/asset-studio/dist/index.html -->
    <style name="AppLightTheme" parent="Theme.Sherlock.Light">
        <item name="android:actionBarItemBackground">@drawable/selectable_background_faq_light</item>
        <item name="android:popupMenuStyle">@style/faq_light_PopupMenu</item>
        <item name="android:dropDownListViewStyle">@style/faq_light_DropDownListView</item>
        <item name="android:actionBarTabStyle">@style/faq_light_ActionBarTabStyle</item>
        <item name="android:actionDropDownStyle">@style/faq_light_DropDownNav</item>
        <item name="android:actionBarStyle">@style/faq_light_solid_ActionBar</item>
        <item name="android:panelBackground">@drawable/menu_hardkey_panel_faq_light</item>
        <item name="android:editTextBackground">@drawable/edit_text_holo_dark</item>
    	<item name="android:fastScrollThumbDrawable">@drawable/fastscroll_thumb_faqr_dark</item>
    </style>
  
	<style name="listviewfastscrollstyle" parent="AppLightTheme">
	    <item name="android:fastScrollTrackDrawable">@drawable/fastscroll_thumb_faqr_dark</item>
	    <item name="android:fastScrollThumbDrawable">@drawable/fastscroll_thumb_faqr_dark</item>
	</style>

    <style name="faq_light_transparent_ActionBar" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">@drawable/ab_transparent_faq_light</item>
        <item name="android:progressBarStyle">@android:style/Widget.ProgressBar.Inverse</item>
    </style>

    <style name="faq_light_PopupMenu" parent="@android:style/Widget.Holo.Light.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_faq_light</item>
    </style>

    <style name="faq_light_DropDownListView" parent="@android:style/Widget.Holo.Light.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_faq_light</item>
    </style>

    <style name="faq_light_DropDownNav" parent="@android:style/Widget.Holo.Light.Spinner">
        <item name="android:background">@drawable/spinner_background_ab_faq_light</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_faq_light</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_faq_light</item>
    </style>

    <style name="faq_light_ProgressBar" parent="@android:style/Widget.Holo.Light.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_faq_light</item>
    </style>

    <!-- BLACK -->
    <style name="AppBlackOverlayTheme" parent="AppBlackTheme">

        <!-- API 11 theme customizations can go here. -->
        <item name="android:windowActionBarOverlay">true</item>
    </style>

    <style name="AppBlackTheme" parent="AppDarkTheme">

        <!-- API 11 theme customizations can go here. -->
        <item name="android:windowBackground">@android:color/black</item>
    </style>

    <!-- DARK -->

    <style name="AppDarkOverlayTheme" parent="AppDarkTheme">

        <!-- API 11 theme customizations can go here. -->
        <item name="android:windowActionBarOverlay">true</item>
    </style>

    <style name="AppDarkTheme" parent="Theme.Sherlock">
        <item name="android:actionBarItemBackground">@drawable/selectable_background_faqr_dark</item>
        <item name="android:popupMenuStyle">@style/faqr_dark_PopupMenu</item>
        <item name="android:dropDownListViewStyle">@style/faqr_dark_DropDownListView</item>
        <item name="android:actionBarTabStyle">@style/faqr_dark_ActionBarTabStyle</item>
        <item name="android:actionDropDownStyle">@style/faqr_dark_DropDownNav</item>
        <item name="android:actionBarStyle">@style/faqr_dark_solid_ActionBar</item>
        <item name="android:panelBackground">@drawable/menu_hardkey_panel_faqr_dark</item>
        <item name="android:editTextBackground">@drawable/edit_text_holo_dark</item>
        <item name="android:windowBackground">@color/faqr_dark_Background</item>
    	<item name="android:fastScrollThumbDrawable">@drawable/fastscroll_thumb_faqr_dark</item>
    </style>

	<!--<style name="listviewfastscrollstyle" parent="AppDarkTheme">-->
	    <!--<item name="android:fastScrollTrackDrawable">@drawable/fastscroll_thumb_faqr_dark</item>-->
	    <!--<item name="android:fastScrollThumbDrawable">@drawable/fastscroll_thumb_faqr_dark</item>-->
	<!--</style>-->

    <style name="faqr_dark_transparent_ActionBar" parent="@android:style/Widget.Holo.ActionBar">
        <item name="android:background">@drawable/ab_transparent_faqr_dark</item>
        <item name="android:progressBarStyle">@android:style/Widget.ProgressBar</item>
    </style>

    <style name="faqr_dark_PopupMenu" parent="@android:style/Widget.Holo.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_faqr_dark</item>
    </style>

    <style name="faqr_dark_DropDownListView" parent="@android:style/Widget.Holo.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_faqr_dark</item>
    </style>

    <style name="faqr_dark_DropDownNav" parent="@android:style/Widget.Holo.Spinner">
        <item name="android:background">@drawable/spinner_background_ab_faqr_dark</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_faqr_dark</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_faqr_dark</item>
    </style>

    <style name="faqr_dark_ProgressBar" parent="@android:style/Widget.Holo.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_faqr_dark</item>
    </style>

</resources>